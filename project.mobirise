range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.2\" condition=\"overlay && bg.type !== 'color'\">\n  </mbr-parameters>\n  <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n  <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n  <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n    <div class=\"row\">\n\n      <div class=\"col-lg-6\">\n\n        <h2 class=\"mbr-section-maintitle mbr-fonts-style mbr-white pb-5\" data-app-selector=\".mbr-section-maintitle\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\">We are recruiting in autumn 2024!</h2>\n\n      </div>\n\n      <div class=\"col-12 col-lg-6\">\n        <h3 class=\"mbr-section-subtitle mbr-fonts-style mbr-white pb-3\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\"><b>We are in the process of recruiting our trainees</b></h3>\n        <p class=\"mbr-fonts-style mbr-text mbr-white mb-5\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn\">In the future, we will present our trainees on this page. If you would like to see yourself highlighted here, check out our open positions.</p>\n\n\n        <div class=\"content-wrap\" mbr-if=\"items > 0\">\n          <div class=\"col-lg-12 my-auto\">\n            <h2 class=\"mbr-section-title m-0 mbr-fonts-style mbr-white\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-section-title, .line, .content-wrap::after\"><a href=\"positions.html\" class=\"text-primary\">OPEN POSITIONS</a></h2>\n          </div>\n          <div class=\"col-12\">\n            <div class=\"line\"></div>\n          </div>\n        </div>\n\n        <div class=\"content-wrap\" mbr-if=\"items > 1\">\n          <div class=\"col-lg-12 my-auto\">\n            <h2 class=\"mbr-section-title m-0 mbr-fonts-style mbr-wh